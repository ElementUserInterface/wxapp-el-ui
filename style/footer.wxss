/*底栏*/
.details-footer {
    z-index: 10;
    display: inline-flex;
    background: #fff;
    border-top: 1px solid #f2f2f2;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    box-shadow: 0 -3px 10px rgba(0, 0, 0, 0.1);
}

.details-footer > view {
    white-space: nowrap;
    padding: 30rpx 0;
    text-align: center;
    font-size: 38rpx;
    line-height: 50rpx;
}
