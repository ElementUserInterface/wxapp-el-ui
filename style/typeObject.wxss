/*类型卡片*/
/*包含房源户型*/
.card-typeobject {
    padding: 20px 25px;
    background-color: #FFF;
    display: inline-flex;
    width: calc(100% - 50px);
}

.card-typeobject > view {
    width: 100%;
}

.card-typeobject-center {
    display: inline-flex;
    align-items: center;
}

.card-typeobject-center > view {
    margin: 0 0 0 10px;
    display: inline-grid;
    width: 100%;
    height: 50px;
}

.card-typeobject-center > view > view {
    display: inline-flex;
    width: 100%;
    text-align: left;
}

.card-typeobject-left {
    width: 110px !important;
}

.card-typeobject-left-assistant {
    color: #333;
    font-size: 30rpx;
}

.card-typeobject-left .card-typeobject-image {
    width: 160rpx;
    height: 158rpx;
    display: block;
    border-radius: .3rem;
    overflow: hidden;
}

.card-typeobject-right {
    width: 18rpx !important;
    display: inline-flex;
    justify-content: center;
    align-items: center;
}

.card-typeobject-right image {
    width: 18rpx;
    height: 35rpx;
}
