@import '../vacancy/vacancy';

.cellswipe-buttons {
  display: inline-flex;
  position: relative;
}

.cellswipe-delete {
  background: red;
  color: #fff;
  padding: 30rpx 40rpx;
  white-space: nowrap;
}

.cellswipe-cancel {
  background: #18b6c1;
  color: #fff;
  padding: 30rpx 40rpx;
  white-space: nowrap;
}

.cellswipe-vacancy {
  margin: 40rpx;
}

.cellswipe-h {
  display: inline-flex;
  align-items: center;
  width: 100%;
  height: 100%;
  white-space: nowrap;
  margin: 0 40rpx;
}
