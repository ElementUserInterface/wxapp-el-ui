.__head-background__ {
   position: relative;
   z-index: 15;
   top: 0;
   left: 0;
   width: 100%;
}

.__head__ {
  overflow: hidden;
}

.__head_up__ {
  position: relative;
  z-index: 4;
  width: 100%;
  height: 90rpx;
}

.__head_stickie__ {
  position: fixed;
  top: 0;
  left: 0;
}

.__head_block__ {
  z-index: 55;
  display: inline-flex;
  width: 100%;
  transition: background-color .25s;
}

.__head_block__ > view {
  width: 100%;
    min-width: 80px;
}

.__head_block_title__ {
  font-weight: 500;
  font-size: calc(11px * 2);
  text-align: center;
  width: 100%;
  min-height: 45px;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  color: #000;
}

.__head_block_title__ > view {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.__head_block_rbtns__ {
  text-align: left;
  width: 100%;
  display: inline-flex;
  align-items: center;
  color: #000;
}

.__head_block_rbtns__ > view:first-child {
  margin: 0 0 0 25px;
}

.__head_block_rbtns__ > view {
  width: 50px;
  height: 100%;
  display: inline-flex;
  align-items: center;
}

.__head_block_rbtns_back__ {
  width: 22rpx;
  height: 40rpx;
}
