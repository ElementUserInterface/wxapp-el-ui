._head_ {
  overflow: hidden;
}

._head_up_ {
  position: relative;
  z-index: 4;
  width: 100%;
  height: 90rpx;
}

._head_stickie_ {
  position: fixed;
  top: 0;
  left: 0;
}

._head_block_ {
  z-index: 55;
  display: inline-flex;
  width: 100%;
  transition: background-color .25s;
}

._head_block_ > view {
  width: 100%;
    min-width: 80px;
}

._head_block_title_ {
  font-weight: 500;
  font-size: calc(11px * 2);
  text-align: center;
  width: 100%;
  min-height: 45px;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  color: #000;
}

._head_block_title_ > view {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

._head_block_rbtns {
  text-align: left;
  width: 100%;
  display: inline-flex;
  align-items: center;
  color: #000;
}

._head_block_rbtns > view:first-child {
  margin: 0 0 0 25px;
}

._head_block_rbtns > view {
  width: 50px;
  height: 100%;
  display: inline-flex;
  align-items: center;
}

._head_block_rbtns_back {
  width: 22rpx;
  height: 40rpx;
}
