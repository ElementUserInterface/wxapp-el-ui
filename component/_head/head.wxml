<view class='_head_' style='height: {{nav.interne.navHeight + "px"}}'>

  <view class='_head_up_' style='margin:{{nav.statusBarHeight}}px 0 0 0; background: {{head.background ? head.background : "#FFF"}}' wx:if='{{stickie}}'></view>

  <view class='_head_block_ {{stickie ? "_head_stickie_" :  ""}}'  style='padding:{{nav.statusBarHeight}}px 0 0 0; background: {{head.style.background ? head.style.background : "#FFF"}}'>

    <view class='_head_block_rbtns'>
      <view bindtap='onBack' wx:if='{{head.back}}'>
        <image class='_head_block_rbtns_back' src='./nav_back.png'></image>
      </view>
      <slot name='nav-left'></slot>
    </view>

    <view class='_head_block_title_' style='color: {{head.style.textColor ? head.style.textColor : "#000"}}'>
      <slot name='nav-tit' style='font-size: {{nav.fontSizeSetting ? nav.fontSizeSetting : ""}}'></slot>
      {{head.title || '标题' + nav.interne}}
    </view>

    <view>
      <slot name='nav-right'></slot>
    </view>

  </view>

</view>